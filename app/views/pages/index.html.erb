      </div>
    </div>
  </div>
</section>
<div class="breakPage"></div>

<section class="contactSec" id="menu8">
  <div class="white">
    <div class="container row">
      <h4>Let us know how we can help you</h4>
      <div class="row">
        <div class="docs-example docs-example-forms">
          <%= form_for(@enquiry1) do |f| %>
            <div class="row">
              <div class="four columns">
                <label for="exampleNameInput">Your Name</label>
                <%= f.text_field :first_name, value:"John",size:"15" %>
                <%= f.text_field :last_name, value:"Smith",size:"16"%>
              </div>
              <div class="eight columns">
                <label for="exampleApptInput">Your reason for contacting us</label>
                <%= f.select :reason,  [['Development','Development'],['Support','Support'],['Hiring','Hiring'],['Other','Other']], {required: 'true', autofocus: 'true', class:'u-full-width'} %>
              </div>
            </div>
            <div class="row contact-icon-row">
              <h4>How would you like us to contact you?</h4>
            </div>
            <div class="row contact-icon">
              <div class="four columns">
                <button class="prefButton"><i class="fa fa-envelope"></i></button>
                <label for="exampleEmailInput">Your email</label>
                <%= f.text_field :email, value:"me@domain.com", class:"u-full-width" %>
              </div>
              <div class="four columns">
                <button class="prefButton"><i class="fa fa-phone"></i></button>
                <label for="examplePhoneInput">Your phone</label>
                <%= f.telephone_field :phone, class:"u-full-width" %>
              </div>
              <div class="four columns">
                <button class="prefButton"><i class="fa fa-user"></i></button>
                <label for="exampleApptInput">Preferred meeting day</label>
                <%= f.date_select :meeting_date %>
                <a href="http://www.google.com/calendar/event?action=TEMPLATE&text=Example%20Event&dates=20160201T010000Z/20160201T020000Z&details=Dev%20House%20X%20Meeting
                &location=CoderFactory%2C%207%20Hudson%20St%2C%20Redfern%2C%20NSW">Add to gCal</a>
              </div>
            </div>
          </div>

          <label for="exampleMessage">Message</label>
          <%= f.text_area :message, class:"u-full-width" %>
          <label class="example-send-yourself-copy">
            <input type="checkbox">
            <span class="label-body">Send a copy to yourself</span>
          </label>
          <div class="actions">
            <%= f.submit "Submit", class:"button-primary"%>
          </div>        
          <% end %>
      </div>
    </div>
  </div>
</section>
<div class="breakPage"></div>

